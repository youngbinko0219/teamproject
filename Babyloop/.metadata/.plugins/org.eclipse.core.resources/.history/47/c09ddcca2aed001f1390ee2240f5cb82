<mapper namespace="com.babyloop.product.repository.IPayinfoMapper">

	<select id="pointInfo"
		parameterType="com.babyloop.product.repository.PayinfoDTO">
	
		SELECT payment_name, user_depoints,
			user_addpoints, create_at
		FROM PAYINFO 
		WHERE user_id=#{user_id}
	
	</select>
	
</mapper>